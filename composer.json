{
  "name": "webudvikleren/laravel-commentable",
  "description": "Easily add comments to any Laravel model",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Webudvikleren\\Commentable\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Webudvikleren\\Commentable\\CommentableServiceProvider"
      ]
    }
  },
  "require": {
    "php": "^8.2",
    "illuminate/support": "^10.0|^11.0|^12.0"
  },
  "require-dev": {
    "orchestra/testbench": "^9.0|^10.0",
    "pestphp/pest": "^3.0",
    "pestphp/pest-plugin-laravel": "^3.0"
  },
  "scripts": {
    "test": "vendor/bin/pest"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
